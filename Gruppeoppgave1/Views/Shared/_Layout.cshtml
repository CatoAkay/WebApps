<!DOCTYPE html>
<html lang="no">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.VY</title>
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/myStyle.css" rel="stylesheet" type="text/css"/>
    <script src="~/Scripts/modernizr-2.8.3.js"></script>
</head>
<body>
<div class="white navbar navbar-inverse navbar-fixed-top">
    <div class="container">
        <div class="col-lg-1">
            <a href="/Home/Index"><img class="gone img-responsive" src="~/IMG/logo.png"/></a>
        </div>
        <div class="col-lg-2 navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            @Html.ActionLink("VY", "Index", "Home", new { area = "" }, new { @class = "black showThis navbar-brand" })
        </div>
        <div class="navbar-collapse collapse">
            <ul class="nav navbar-nav">
            </ul>
        </div>
    </div>
</div>


<div class="container body-content">
    @*<img class="size" src="~/IMG/logo.png"/>*@
    @RenderBody()
    <hr/>
    <footer>
        <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
    </footer>
</div>
<script src="~/Scripts/jquery-3.3.1.min.js"></script>
<script src="~/Scripts/site/main.js"></script>
<script src="~/Scripts/bootstrap.min.js"></script>
</body>
</html>